{
  "name": "treewalk",
  "version": "0.1.0",
  "description": "树遍历",
  "keywords": [
    "component",
    "treewalk",
    "tree"
  ],
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf examples lib/* dist",
    "doc": "jsdoc2md src/**/*.js > docs/API.md",
    "dev": "cross-env NODE_ENV=BROWSER DEBUG=omelette:webpack:dev,omelette:webpack:helper supervisor -w builder builder/webpack.server.js",
    "build:examples": "cross-env DEBUG=omelette:webpack,omelette:webpack:prod,omelette:webpack:helper webpack --config builder/webpack.config.examples.js --progress --display-error-details --colors",
    "build:prod": "cross-env DEBUG=omelette:webpack,omelette:webpack:prod,omelette:webpack:helper webpack --config builder/webpack.config.prod.js --progress --display-error-details --colors",
    "build": "npm run clean && npm run build:copy && npm run compile && npm run build:prod",
    "build:copy": "cp -r ./src/* ./lib",
    "compile": "babel src -d lib",
    "lint": "eslint ./src",
    "karma": "karma start karma.conf.js",
    "test": "karma start --single-run --no-auto-watch karma.conf.js"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:zhangmhao/walktree.git"
  },
  "author": {
    "name": "andrew",
    "email": "zhangmhao@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-static": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "babylon": "^6.17.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.0",
    "debug": "^2.6.0",
    "es6-promise": "^4.1.1",
    "eslint": "^3.14.0",
    "eslint-config-egg": "^3.2.0",
    "eslint-loader": "^1.6.1",
    "express": "^4.14.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "ip": "^1.1.4",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "killable": "^1.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "mock-browser": "^0.92.14",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "recast": "^0.12.5",
    "redux": "^3.6.0",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supervisor": "^0.12.0",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-isomorphic-tools": "^3.0.2"
  }
}
